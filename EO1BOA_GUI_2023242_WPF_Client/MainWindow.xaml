<Window x:Class="EO1BOA_GUI_2023242_WPF_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EO1BOA_GUI_2023242_WPF_Client"
        mc:Ignorable="d"
        Title="MainBakeryWindow" Height="233" Width="229">
    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>
    <Grid Margin="0,0,0,-16" Background="White">
        <StackPanel>
            <Button Width="200" Height="50" Content="Bakery" Margin="0,10,0,10" Command="{Binding GetBakeryCommand}" FontSize="19" Background="Aqua"></Button>
            <Button Width="200" Height="50" Content="Bread" Margin="0,10,0,10" Command="{Binding GetBreadCommand}" FontSize="19" Background="Aqua"></Button>
            <Button Width="200" Height="50" Content="Oven" Margin="0,10,0,10" Command="{Binding GetOvensCommand}" FontSize="19" Background="Aqua"></Button>
        </StackPanel>
    </Grid>
</Window>